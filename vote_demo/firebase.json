{
  "hosting": {
    "predeploy": "flutter build web --wasm --no-strip-wasm",
    "site": "vote-dart",
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*"
    ],
    "headers": [
      {
        "source": "**/*",
        "headers": [
          {
            "key": "cross-origin-opener-policy",
            "value": "same-origin"
          },
          {
            "key": "cross-origin-embedder-policy",
            "value": "require-corp"
          }
        ]
      }, {
        "source": "/?(\\?*)",
        "headers": [
          {
            "key": "Link",
            "value": "</assets/FontManifest.json>; rel=preload; as=fetch; crossorigin"
          }
        ]
      }
    ]
  }
}
